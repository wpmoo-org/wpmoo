{
  "name": "wpmoo/wpmoo",
  "description": "WPMoo \u2014 WordPress Micro Object-Oriented Framework (PHP 7.4+)",
  "license": "GPL-2.0-or-later",
  "homepage": "https://wpmoo.org",
  "authors": [
    {
      "name": "WPMoo",
      "email": "hello@wpmoo.org",
      "homepage": "https://wpmoo.org/"
    }
  ],
  "keywords": [
    "wordpress",
    "wp-cli",
    "framework",
    "micro-framework",
    "php",
    "php7.4",
    "php8.0",
    "php8.1",
    "php8.2",
    "php8.3",
    "oop",
    "object-oriented"
  ],
  "support": {
    "issues": "https://github.com/wpmoo/wpmoo/issues",
    "source": "https://github.com/wpmoo/wpmoo/"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.1.x-dev"
    }
  },
  "type": "wordpress-framework",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "WPMoo\\": "framework/",
      "WPMoo\\Samples\\": "src/samples/"
    }
  },
  "require-dev": {
    "wpmoo/wpmoo-cli": "dev-dev",
    "scssphp/scssphp": "^1.12"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}