includes:
    - vendor/szepeviktor/phpstan-wordpress/extension.neon
    - phpstan-baseline.neon

parameters:
    level: 6
    phpVersion: 70400
    paths:
        - src
    excludePaths:
        - vendor
        - tests
        - src/CLI
    reportUnmatchedIgnoredErrors: false
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        -
            message: '#Method WPMoo\\Admin\\UI\\Panel::make\(\) should return static#'
            path: src/Admin/UI/Panel.php
